{
  "name": "eweb",
  "version": "1.0.0",
  "description": "electron web browser",
  "main": "eweb.js",
  "scripts": {
    "build": "node_modules/.bin/electron-builder"
  },
  "build": {
    "appId": "torapp.eweb",
    "productName": "eweb",
    "publish": "github",
    "files": [
      "eweb.js",
      "addressbar.html",
      "package.json"
    ],
    "directories": {
      "output": "release"
    },
    "dmg": {
      "contents": [
        { "x": 110, "y": 150 },
        { "x": 240, "y": 150, "type": "link", "path": "/Applications" }
      ]
    },
    "nsis": {
      "createDesktopShortcut": "always"
    }
  },
  "dependencies": {
    "electron":""
  },
  "devDependencies": {
    "electron-builder": "",
    "electron-prebuilt-compile": ""
  }
}
